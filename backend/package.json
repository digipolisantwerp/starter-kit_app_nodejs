{
  "name": "starter-kit",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "env-cmd --no-override ./environment/local.env nodemon src/index.js",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "lint": "eslint **/*.js",
    "test": "env-cmd ./environment/test.env --no-override nyc mocha --recursive  --reporter=spec --timeout=15000 --slow 2 --require @babel/register --require @babel/polyfill  --require ./test/setup.test.js --exit && npm run lint",
    "test:coverage": "env-cmd ./environment/test.env  ./node_modules/.bin/nyc  --reporter=lcovonly --reporter=text ./node_modules/.bin/mocha --recursive --timeout=15000 --slow 2 --require @babel/register --require @babel/polyfill --require ./test/setup.test.js --exit",
    "test-build": "env-cmd ./environment/test.env nyc mocha --recursive --reporter=mocha-bamboo-reporter --timeout=15000 --slow 2 --require @babel/register  --require @babel/polyfill --require ./test/setup.test.js --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@digipolis/auth": "^1.1.0",
    "body-parser": "~1.18.2",
    "digipolis-response": "~1.0.1",
    "express": "~4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.6.11",
    "uuid": "~3.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "~7.0.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "~7.0.0",
    "chai": "^4.2.0",
    "env-cmd": "^8.0.2",
    "eslint": "~4.19.1",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-import": "~2.11.0",
    "mocha": "~5.1.1",
    "nodemon": "^1.18.10",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "sinon": "^7.4.1",
    "supertest": "^3.0.0"
  }
}
